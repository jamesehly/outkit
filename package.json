{
  "name": "outkit",
  "version": "0.1.2",
  "description": "Outkit - Easily create custom UI components.",
  "author": "James Ehly",
  "license": "MIT",
  "main": "dist/outkit.js",
  "typings": "dist/outkit.d.ts",
  "scripts": {
    "build": "rimraf ./dist && webpack && uglifyjs ./dist/outkit.js -c -m -o ./dist/outkit.min.js --source-map ./dist/outkit.min.js.map --comments /Copyright/",
    "docs": "typedoc --out docs/api/ src/js/",
    "preversion": "npm test",
    "release": "standard-version",
    "serve": "webpack-dev-server --open --hot",
    "start": "npm run serve",
    "test": "karma start"
  },
  "devDependencies": {
    "@types/jasmine": "^2.5.43",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "jasmine": "^2.5.3",
    "karma": "^1.5.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-typescript": "^3.0.0-beta.2",
    "karma-webpack": "^2.0.3",
    "phantomjs": "^2.1.7",
    "rimraf": "^2.6.1",
    "standard-version": "^4.0.0",
    "ts-loader": "^2.0.0",
    "typedoc": "^0.5.7",
    "typescript": "^2.1.5",
    "uglifyjs": "^2.4.10",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0"
  }
}
